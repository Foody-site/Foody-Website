{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "framework": "vite",
    "env": {
        "DISABLE_ESLINT_PLUGIN": "true"
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "functions": {
        "app/api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    }
}